class GiftCard
types
	
values
	public GiftValues : set of nat1 = {10, 20, 40};

instance variables
	id: nat;
	value: nat1;
	
	inv value in set GiftValues;
	
operations
	public GiftCard(i: nat, v: nat) res: GiftCard ==
		(id := i; value := v; return self;)
		pre true
		post id = i and value = v;
		
	public pure getValue() res:nat1 ==
		return value
		pre true
		post true;

functions

