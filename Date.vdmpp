-- A simple date class with only day, year and month
class Date
instance variables
	year : nat1;
	month: nat1;
	day : nat1;
	inv day <= 31;
	inv year >= 2000;
	inv month <= 12;

operations
	public Date: nat1 * nat1 * nat1 ==> Date 
	Date(d,m,y) ==
		(
		day := d;
		month := m;
		year := y;
		return self;
		)
		pre true
		post day=d and month=m and year = y and RESULT=self;
end Date